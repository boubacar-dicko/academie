                <th>DateNaiss</th>
                <td>{{ inscription.dateNaiss ? inscription.dateNaiss|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('inscription_index') }}">back to list</a>

