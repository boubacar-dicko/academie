{% extends 'base.html.twig' %}

{% block title %}Inscription index{% endblock %}

{% block body %}
    <h1>Liste des Eleves Inscrits</h1>

    <table class="table">
        <thead>
        <tr>
            <!--th>Id</th-->
            <th>Nom</th>
            <th>Date Naissance</th>
            <th>Nom Etablissement</th>
            <th>Nom Examen</th>
            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for inscription in inscriptions %}
            <tr>
                <!--td>{{ inscription.id }}</td-->
                <td>{{ inscription.nom }}</td>
                <td>{{ inscription.dateNaiss ? inscription.dateNaiss|date('Y-m-d') : '' }}</td>
                <td>{{ inscription.nomEtablissement }}</td>
                <td>{{ inscription.nomExamen }}</td>
                <td>
                    <a href="{{ path('inscription_show', {'id': inscription.id}) }}">show</a>
                    <!--a href="{{ path('inscription_edit', {'id': inscription.id}) }}">edit</a-->
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <!--a href="{{ path('inscription_new') }}">Create new</a-->
{% endblock %}
