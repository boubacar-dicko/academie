{% extends 'base.html.twig' %}

{% block title %}Examen index{% endblock %}

{% block body %}
    <h1>Examen index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>NomExamen</th>
                <th>Date</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for examan in examens %}
            <tr>
                <td>{{ examan.id }}</td>
                <td>{{ examan.nomExamen }}</td>
                <td>{{ examan.date ? examan.date|date('Y-m-d') : '' }}</td>
                <td>
                    <a href="{{ path('examen_show', {'id': examan.id}) }}">show</a>
                    <a href="{{ path('examen_edit', {'id': examan.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('examen_new') }}">Create new</a>
{% endblock %}
